.row-fluid#home

  .row-fluid
    .span4
      .panel.clearfix
        .row-fluid
          .span10.offset1
            / = render "pages/home/careers"
            / = render "pages/home/hiring"
            = render "pages/home/lobby_closed"

      .panel.clearfix
        .row-fluid
          .span10.offset1.text-center{style: "padding-top:15px; padding-bottom:15px"}
            .video-container
              %iframe.ad-video{width: "46", height: "35", allowfullscreen: true, frameborder: "0", src: 'https://www.youtube.com/embed/K-OcTc7rH8c?&modestbranding=0&controls=1&rel=0&showinfo=0'}

    .span4
      .hero-unit.clearfix.text-center
        = render partial: 'carousel', locals: {slides: @slides, employees: @employees}
      .panel.clearfix
        .row-fluid
          .span10.offset1
            / = render "pages/home/careers"
            = render "pages/home/hiring"

    .span4
      .panel.clearfix
        .row-fluid
          .span10.offset1.text-center{style: "padding-top:15px"}
            /= render "pages/home/cycle_show"
            = render "pages/home/bee"

      .panel.clearfix
        .row-fluid
          .span10.offset1.text-center{style: "padding-top:15px"}
            = render "pages/home/facebook"

.row-fluid#home
