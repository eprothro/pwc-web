.row-fluid#home
  .span4
    .panel.clearfix
      .row-fluid
        .span10.offset1.text-center
          %p
            = link_to album_path(Album.find_by_slug('involvement-around-town')), title: 'See more photos of PW involvement around town' do
              - image_tag 'bee/cover.jpg'
          %p.text-center
            Visit
            = link_to('our Facebook page', 'https://www.facebook.com/ProthroWilhelmi', target: '_blank')
            to stay current with PW news and community involvement!

    .panel.clearfix{style: "min-height: 156px"}
      .row-fluid
        .span6.text-center
          = render 'employees/avatar', employee: Employee.find_by_name('Tom Prothro')
        .span6
          %p Congratulations to Tom Prothro – Governor Rick Perry has named Tom Prothro of Tyler as chair of the Texas State Board of Public Accountancy!
  .span4
    .hero-unit.clearfix.text-center
      = render partial: 'carousel', locals: {slides: @slides, employees: @employees}

  .span4
    .panel.clearfix
      .row-fluid
        .span6.text-center
          = render 'employees/avatar', employee: Employee.find_by_name('Kristi Moore')
        .span6
          %p
            PW is pleased to announce that Kristi Moore, CPA has
            = link_to('become a partner', asset_path('Kristi Moore | Newspaper Announcement.pdf'), target: '_blank')
            in the firm!
    .panel.clearfix
      .row-fluid
        .span10.offset1.text-center
          .video-container
            %iframe.ad-video{width: "46", height: "35", allowfullscreen: true, frameborder: "0", src: 'http://www.youtube.com/embed/TTWmBAZWj9g?&modestbranding=0&controls=1&rel=0&showinfo=0'}
      %p.text-center{style: "margin-top: 5px"} "Celebrating 20 years!"

.row-fluid#home
  .span12
    .dedication
      %em
        .preable This website has been dedicated in loving memory of
        .name Ashley Prothro Trant
        .title Original Designer
        .date February 18, 1982 – March 27, 2013
